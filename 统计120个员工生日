#include<stdio.h>
#include<stdlib.h>
void main()
{
	int count[13];
	int month[121];
	int i;
	for(i=1;i<=12;i++)
	{
		count[i]=0;
	}
	for(i=0;i<120;i++)
	{
		scanf("%d",&month[i]);
	}
	for(i=0;i<120;i++)
	{
		count[month[i]]++;
	}
	for(i=1;i<=12;i++)
	{
		printf("%3d",count[i]);
	}
	system("pause");
}
